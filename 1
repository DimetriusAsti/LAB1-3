#include <iostream>
using namespace std;

int main() {
    int const row = 5;
    int arr[row][row] = {};
    int num = 1;
    
    for (int i = 0; i < row; i++){
        for (int j = 0; j < row; j++){
            if ((i + j) % 2 != 0){
                arr[i][j] = 0;
            }
            else {
                arr[i][j] = num;
                num += 1;
                if (num > 9){
                    num = 1;
                }
                
                }
            }
        }
        
    for (int i = 0; i < row; i++){
        for (int j = 0; j < row; j++){
            cout << arr[i][j] << " ";
        }
    cout << endl;
    }
    return 0;
}
