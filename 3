#include <iostream>
using namespace std;
int main() {
	int a[] = { 0, 0, 0, 0, 0, 0, 0, 1 };
	int j = 0, i = 0;
	while (i < 8) {
		if (a[i] == 0) {
			i += 1;
		}
		else {
			a[j] = a[i];
			i += 1;
			j += 1;
		}
	}
	for (j; j < 8; j++) {
		a[j] = 0;
	}

	for (int i = 0; i < 8; i++) {
		cout << a[i] << " ";
	}
	return 0;
}
